{
  "manifest_version": 2,
  "name": "Life in Weeks",
  "version": "1.0",
  "description": "Visualize your life in weeks",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "applications": {
    "gecko": {
      "id": "life-in-weeks@example.com",
      "strict_min_version": "57.0"
    }
  }
} 